# wengwengweng

cmd + shift - 2     : screencapture -w $HOME/Desktop/(date +%s).png
cmd + shift - 3     : screencapture $HOME/Desktop/(date +%s).png
cmd + shift - 4     : screencapture -i $HOME/Desktop/(date +%s).png

shift + alt - left : yabai -m window --move rel:-64:0
shift + alt - right : yabai -m window --move rel:64:0
shift + alt - up : yabai -m window --move rel:0:-64
shift + alt - down : yabai -m window --move rel:0:64

