# wengwengweng

[core]
	excludesfile = ~/.gitignore_g
	pager = less -+FX

[user]
	email = tga@enemyspy.xyz
	name = tga

[alias]
	conflicts = diff --name-only --diff-filter=U
	root = rev-parse --show-toplevel
	file = log --follow -p --
	slog = log --oneline
	commitf = commit --amend --no-edit

[credential]
	helper = store

[pull]
	rebase = false

