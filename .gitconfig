# wengwengweng

[core]
	excludesfile = ~/.gitignore_g
	pager = less -+FX

[user]
	email = tga@space55.xyz
	name = tga

[alias]
	conflicts = diff --name-only --diff-filter=U
	root = rev-parse --show-toplevel
	file = log --follow -p --
	slog = log --oneline
	flog = log --pretty=fuller
	logf = log -p
	commitf = commit --amend --no-edit
	new = checkout -b

[credential]
	helper = store

[pull]
	rebase = false

